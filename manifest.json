{
    "manifest_version" : 2,
    "name"          : "RubyDoc Search",
    "description"   : "Search ruby-doc.org within Chrome omnibox itself",
    "version"       : "1.0.0",
    "omnibox"       : { "keyword": "rdoc" },
    "icons"         : {
        "16" : "mozilla-16.ico",
        "48" : "mozilla-48.png",
        "128": "mozilla-128.jpg"
    },
    "background"    : {
        "scripts": [
            "jquery-1.7.2.min.js",
            "underscore-1.3.3.min.js",
            "mdn_search.js"
        ]
    }
}